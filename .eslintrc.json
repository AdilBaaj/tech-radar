{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
        "no-redeclare": "off",
        "no-undef": "off",
        "no-unused-vars": "off"
    }
}
